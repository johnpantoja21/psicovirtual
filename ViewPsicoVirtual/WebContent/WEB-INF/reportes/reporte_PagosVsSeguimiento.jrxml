	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="FECHA_INICIO" class="java.util.Date"/>
	<parameter name="FECHA_FIN" class="java.util.Date"/>
	<parameter name="USUARIOS" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT TPAGOS_LIQUIDACION.id_liqui,valor,cedulagirador,
(SELECT rtrim (xmlagg (xmlelement (e,gestion || '-----')).extract ('//text()'), '-----') FROM SEGUIMIENTO WHERE UPPER(USUARIO) IN
( $P!{USUARIOS})

AND TRUNC(COMPROMISO_PAGO)>=TRUNC($P{FECHA_INICIO})
AND TRUNC(COMPROMISO_PAGO) <=TRUNC($P{FECHA_FIN}) and cedula=cedulagirador) as gestion
,
(SELECT rtrim (xmlagg (xmlelement (e,usuario || '-----')).extract ('//text()'), '-----') FROM SEGUIMIENTO WHERE UPPER(USUARIO) IN
( $P!{USUARIOS})


AND TRUNC(COMPROMISO_PAGO)>=$P{FECHA_INICIO}
AND TRUNC(COMPROMISO_PAGO) <=$P{FECHA_FIN} and cedula=cedulagirador) as usuario,

(SELECT rtrim (xmlagg (xmlelement (e,DIA_CONTACTO || '-----')).extract ('//text()'), '-----') FROM SEGUIMIENTO WHERE UPPER(USUARIO) IN
( $P!{USUARIOS})

AND TRUNC(COMPROMISO_PAGO)>=TRUNC($P{FECHA_INICIO})
AND TRUNC(COMPROMISO_PAGO) <=TRUNC($P{FECHA_FIN})  and cedula=cedulagirador) as DIA_CONTACTO,
(SELECT rtrim (xmlagg (xmlelement (e,COMPROMISO_PAGO || '-----')).extract ('//text()'), '-----') FROM SEGUIMIENTO WHERE UPPER(USUARIO) IN
( $P!{USUARIOS})


AND TRUNC(COMPROMISO_PAGO)>=TRUNC($P{FECHA_INICIO})
AND TRUNC(COMPROMISO_PAGO) <=TRUNC($P{FECHA_FIN}) and cedula=cedulagirador) as COMPROMISO_PAGO



FROM TPAGOS_LIQUIDACION, TPAGOS_PAGO, SEGUIMIENTO
WHERE TPAGOS_LIQUIDACION.CEDULAGIRADOR=SEGUIMIENTO.CEDULA
AND TPAGOS_LIQUIDACION.ID_LIQUI=TPAGOS_PAGO.ID_LIQUI
AND TPAGOS_PAGO.FECHA_PAGO >=TRUNC($P{FECHA_INICIO})
AND TPAGOS_PAGO.FECHA_PAGO <=TRUNC($P{FECHA_FIN})
AND  CEDULAGIRADOR IN(SELECT CEDULA FROM SEGUIMIENTO WHERE UPPER(USUARIO) IN
( $P!{USUARIOS})

AND TRUNC(SEGUIMIENTO.COMPROMISO_PAGO)>=TRUNC($P{FECHA_INICIO})
AND TRUNC(SEGUIMIENTO.COMPROMISO_PAGO) <=TRUNC($P{FECHA_FIN})
)
GROUP BY   TPAGOS_LIQUIDACION.id_liqui,valor,cedulagirador]]>
	</queryString>
