	<parameter name="cedula" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT ID_CODA,ID_GIRA,ID_ESCO,CAST(fecha_consulta AS DATE) as fecha_consulta,
CAST(VIGENTE_HASTA AS DATE) as vigente_hasta, vigencia_disminuida, campo_1,
tipo_respuesta, user_id, masivo, producto, apellidodigitado, id_prod_web,
cod_afiliado,xml
FROM Tdata_Consulta_Datacredito
WHERE Id_Gira =
  (SELECT Id_Gira
  FROM tdata_girador
  WHERE  No_Documento = to_char($P{cedula})
  )
ORDER BY fecha_consulta DESC]]>
	</queryString>
