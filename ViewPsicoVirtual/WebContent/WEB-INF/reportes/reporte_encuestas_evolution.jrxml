	<parameter name="FECHA_INICIAL" class="java.util.Date"/>
	<parameter name="FECHA_FINAL" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT Encuesta_1.IdTransaccion,
       Encuesta_1.IdTrans_Original,
       Encuesta_1.Fecha_Hora,
       Encuesta_1.Telefono,
       Encuesta_1.Resp_1,
       Encuesta_1.Resp_2,
       Encuesta_1.Resp_3,
       (select NOMBRE + ' ' + apellido1 agente
          from dbo.usuario
         where  idUsuario = TRANSACCION.idAgente ) Agente
 FROM Encuesta_1,
       TRANSACCION
 where Encuesta_1.IdTrans_Original = TRANSACCION.idTransaccion
 and  CAST(Encuesta_1.fecha_hora as date) between  $P{FECHA_INICIAL} and  $P{FECHA_FINAL}
UNION
SELECT Encuesta_1.IdTransaccion,
       Encuesta_1.IdTrans_Original,
       Encuesta_1.Fecha_Hora,
       Encuesta_1.Telefono,
       Encuesta_1.Resp_1,
       Encuesta_1.Resp_2,
       Encuesta_1.Resp_3,
       (select NOMBRE + ' ' + apellido1 agente
          from dbo.usuario
         where  idUsuario = TbHistoFtransacciones.idAgente ) Agente
 FROM Encuesta_1,
       TbHistoFtransacciones
 where Encuesta_1.IdTrans_Original = TbHistoFtransacciones.idTransaccion
 and    Encuesta_1.fecha_hora between $P{FECHA_INICIAL} and DATEADD(day,1,$P{FECHA_FINAL})
 and    TbHistoFtransacciones.tcreacion between $P{FECHA_INICIAL} and DATEADD(day,1,$P{FECHA_FINAL})]]>
	</queryString>
