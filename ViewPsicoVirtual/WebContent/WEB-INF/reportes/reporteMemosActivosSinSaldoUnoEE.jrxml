<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="reporteMemosInactivosConSaldoUnoEE" language="groovy" pageWidth="678" pageHeight="842" columnWidth="638" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="2a0f5e33-1049-4569-b54d-8c42c83ef987">
	<property name="ireport.zoom" value="2.0"/>
	<property name="ireport.x" value="82"/>
	<property name="ireport.y" value="0"/>
	<parameter name="FECHA_INICIAL" class="java.util.Date"/>
	<parameter name="FECHA_FINAL" class="java.util.Date"/>
	<queryString>
		<![CDATA[select m.numero AS NUMERO_MEMO ,m.valor AS VALOR_MEMO,  sum(a.a_capital)  as TOTAL_ABONOS , (m.valor-sum(a.a_capital)) as SALDO_COBRANZAS, F_SALDO_UNOOEE_MEMO(m.numero) as SALDO_UNOEE,
(m.valor-sum(a.a_capital))-F_SALDO_UNOOEE_MEMO(m.numero) as DIFERENCIA_SALDO
from memos m , table(f_abonos_memo(m.numero)) a
where id_estado='1'
and trunc(fecmemo) >= $P{FECHA_INICIAL}
and trunc(fecmemo) <= $P{FECHA_FINAL}
AND m.NOTA !=3
and (m.TRANSFER is null or m.transfer = 2  or m.transfer = 0)
AND F_SALDO_UNOOEE_MEMO(m.numero)=0
group by to_char(fecmemo,'YYYY'), m.numero ,m.valor
having ((m.valor-sum(a.a_capital))-F_SALDO_UNOOEE_MEMO(m.numero) )>0]]>
	</queryString>
	<field name="NUMERO_MEMO" class="java.math.BigDecimal"/>
	<field name="VALOR_MEMO" class="java.math.BigDecimal"/>
	<field name="TOTAL_ABONOS" class="java.math.BigDecimal"/>
