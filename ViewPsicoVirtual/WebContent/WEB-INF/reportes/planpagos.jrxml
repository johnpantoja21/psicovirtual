	<queryString>
		<![CDATA[SELECT	'CHEQUE POSFECHADO' AS PRODUCTO,
               PGIRDAT.POSCON as NUMERO_AVAL,
               AFIDATE1.AFICON as AFICON,
               AFIDATE1.AFICODSCC as AFICODSCC,
               AFIDATE1.AFICODSCC||CASE WHEN LENGTH(CAST(AFIDATE1.AFICON AS VARCHAR(6)))<=4 THEN '0'||CAST(AFIDATE1.AFICON AS VARCHAR(6)) ELSE CAST(AFIDATE1.AFICON AS VARCHAR(6)) END||CASE WHEN LENGTH(CAST(AFIDATE1.AFINUMSUC AS VARCHAR(3)))<=1 THEN '0'||CAST(AFIDATE1.AFINUMSUC AS VARCHAR(6)) ELSE CAST(AFIDATE1.AFINUMSUC AS VARCHAR(6)) END AS CODIGO_AFILIADO,
               PGIRDAT.POSDIDNOM as GIRADOR,
               PGIRDAT.POSDIR as DIRECCION,
               PGIRDAT.POSTEL1 as TELEFONO_1,
               PGIRDAT.POSTEL2 as TELEFONO_2,
               PGIRDAT.POSTELCEL as POSTELCEL,
               PCONPOS1.POSNUMCHE as CHEQUE,
               PCONPOS1.POSVALCHE as VALOR,
               PCONPOS1.POSFECCON as FECHA_VENCIMINETO,
               PGIRDAT.POSDIDNUM as NUMERO_IDENTIFICACION,
               AFIDATE1.AFIRAZSOC as NOMBRE_AFILIADO,
               AFIDATE1.AFINOMCOM as TIENDA,
               PGIRDAT.POSFEC AS FECHA   ,
               PGIRDAT.BANCOD AS BANCO,
               PGIRDAT.POSNUMCUN AS NUMERO_CUENTA,
               PGIRDAT.POSDOCCOD AS TIPO_DOCUMENTO,
	     PCONPOS1.POSCANLIN AS CUOTA
          FROM SIPCDTA0.PGIRDAT PGIRDAT,
          SIPCDTA0.PCONPOS1 PCONPOS1,
          SIPCDTA0.AFIDATE1 AFIDATE1
         where PGIRDAT.rsecod = 3
         AND PGIRDAT.POSCON = PCONPOS1.POSCON
         AND PGIRDAT.AFICON = AFIDATE1.AFICON
         AND PGIRDAT.AFINUMSUC = AFIDATE1.AFINUMSUC
         AND PGIRDAT.AFICODSCC = AFIDATE1.AFICODSCC
         AND PGIRDAT.POSCON      =$P{NUMERO_APROBACION}
         AND AFIDATE1.AFIDIDNUM  =$P{NIT}
	AND  PGIRDAT.AFINUMSUC =$P{SUCURSAL}
	UNION
	SELECT 'CHEQUE AL DIA' AS PRODUCTO,
               fgirador.GIRCON as NUMERO_AVAL,
               AFIDATE1.AFICON as AFICON,
               AFIDATE1.AFICODSCC as AFICODSCC,
               AFIDATE1.AFICODSCC||CASE WHEN LENGTH(CAST(AFIDATE1.AFICON AS VARCHAR(6)))<=4 THEN '0'||CAST(AFIDATE1.AFICON AS VARCHAR(6)) ELSE CAST(AFIDATE1.AFICON AS VARCHAR(6)) END||CASE WHEN LENGTH(CAST(AFIDATE1.AFINUMSUC AS VARCHAR(3)))<=1 THEN '0'||CAST(AFIDATE1.AFINUMSUC AS VARCHAR(6)) ELSE CAST(AFIDATE1.AFINUMSUC AS VARCHAR(6)) END AS CODIGO_AFILIADO,
               fgirador.GIRDIDNOM as GIRADOR,
               fgirador.GIRDIR as DIRECCION,
               fgirador.GIRTEL1 as TELEFONO_1,
               fgirador.GIRTEL2 as TELEFONO_2,
               fgirador.GIRTELCEL as POSTELCEL,
               fgirador.GIRNUMCHE as CHEQUE,
               fgirador.GIRVALCHE as VALOR,
               fgirador.GIRFEC as FECHA_VENCIMINETO,
               fgirador.GIRDIDNUM as NUMERO_IDENTIFICACION,
               AFIDATE1.AFIRAZSOC as NOMBRE_AFILIADO,
               AFIDATE1.AFINOMCOM as TIENDA,
               fgirador.GIRFEC AS FECHA   ,
               fgirador.BANCOD AS BANCO,
               fgirador.GIRNUMCUN AS NUMERO_CUENTA,
               fgirador.GIRDOCCOD AS TIPO_DOCUMENTO,
	     1 AS CUOTA
          FROM SIPCDTA0.fgirador fgirador,
          SIPCDTA0.AFIDATE1 AFIDATE1
         where fgirador.rsecod = 3
         AND fgirador.AFICON = AFIDATE1.AFICON
         AND fgirador.AFINUMSUC = AFIDATE1.AFINUMSUC
         AND fgirador.AFICODSCC = AFIDATE1.AFICODSCC
         AND fgirador.GIRCON    =$P{NUMERO_APROBACION}
         AND AFIDATE1.AFIDIDNUM =$P{NIT}
         AND  fgirador.AFINUMSUC=$P{SUCURSAL}
ORDER BY CUOTA]]>
	</queryString>
